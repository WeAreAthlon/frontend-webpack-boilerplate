    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:WeAreAthlon/frontend-webpack-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/WeAreAthlon/frontend-webpack-boilerplate/issues",
    "email": "plamen@athlonsofia.com"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "autoprefixer": "^9.6.0"
  },
